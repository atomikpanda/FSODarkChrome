{
  "manifest_version": 2,
  "name": "FSO Dark",
  "version": "0.0.0.3",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
	  "default_icon": "icon.png"
	},
	"background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://online.fullsail.edu/"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}